<main class="account-management">
  <h1><%= title %></h1>

  <!-- FLASH MESSAGE -->
  <% if (messages && messages.length > 0) { %>
    <div class="notice">
      <%= messages %>
    </div>
  <% } %>

  <% if (errors && errors.length > 0) { %>
    <div class="errors">
      <%= errors %>
    </div>
  <% } %>

  <p>You're logged in</p>
</main>
